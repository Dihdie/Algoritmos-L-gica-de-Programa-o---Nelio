algoritmo "Exercicios - Soma"
// Função :
// Autor :  Diego
// Data : 30/01/2024
// Seção de Declarações 
var
     x,y,soma:inteiro
inicio
      escreval("Digite o valor de x: ")
      leia(x)
      
      escreval("Digite o valor de y: ")
      leia(y)
      
      soma <- x + y
      escreval("Soma: " , soma)
fimalgoritmo
